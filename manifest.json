{
  "manifest_version": 2,
  "name": "9GAG video controls",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://*.9gag.com/", "*://*.9gag.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons":{
    "128":"icon_video.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
