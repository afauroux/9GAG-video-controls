{
  "manifest_version": 2,
  "name": "9GAG video controls",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://*.9gag.com/"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon2.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
